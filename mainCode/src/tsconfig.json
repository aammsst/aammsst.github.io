{
  "compilerOptions": {
    "target": "es2016",                                  
    "module": "commonjs",
    "resolveJsonModule": true,
    "allowJs": true,
    "removeComments": true,
    "outDir": "../build/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
